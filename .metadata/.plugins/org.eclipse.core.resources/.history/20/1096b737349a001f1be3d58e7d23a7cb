package gerenciadorEscola;
import java.util.ArrayList;
import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int menu=0;
		
		while (menu != 15)
		{
			System.out.println("================\n"
					+ "MENU PRINCIAPAL\n"
					+ "================");
			menu = scan.nextInt();
		}

	}

}
